table 'Calculation group'
	lineageTag: f8e90088-1832-410f-af92-c3ec07df9de7

	calculationGroup

		calculationItem YTD = CALCULATE(SELECTEDMEASURE(),DATESYTD('Calendar'[Date]))

		calculationItem MTD = CALCULATE(SELECTEDMEASURE(),DATESMTD('Calendar'[Date]))

		calculationItem QTD = CALCULATE(SELECTEDMEASURE(),DATESQTD('Calendar'[Date]))

		calculationItem PreviousPeriod =
				
				VAR SelectedStartDate = MIN('Calendar'[Date])
				VAR SelectedEndDate = MAX('Calendar'[Date])
				VAR DaysInSelectedPeriod = DATEDIFF(SelectedStartDate, SelectedEndDate, DAY) + 1
				VAR PreviousPeriodStartDate = SelectedStartDate - DaysInSelectedPeriod
				VAR PreviousPeriodEndDate = SelectedEndDate - DaysInSelectedPeriod
				RETURN
				CALCULATE(
				    SELECTEDMEASURE(),
				    FILTER(
				        ALL('Calendar'[Date]),
				        'Calendar'[Date] >= PreviousPeriodStartDate &&
				        'Calendar'[Date] <= PreviousPeriodEndDate
				    )
				)

		calculationItem 'PP %' =
				VAR SelectedStartDate = MIN('Calendar'[Date])
				VAR SelectedEndDate = MAX('Calendar'[Date])
				VAR DaysInSelectedPeriod = DATEDIFF(SelectedStartDate, SelectedEndDate, DAY) + 1
				VAR PreviousPeriodStartDate = SelectedStartDate - DaysInSelectedPeriod
				VAR PreviousPeriodEndDate = SelectedEndDate - DaysInSelectedPeriod
				RETURN
				DIVIDE(SELECTEDMEASURE(),CALCULATE(
				    SELECTEDMEASURE(),
				    FILTER(
				        ALL('Calendar'[Date]),
				        'Calendar'[Date] >= PreviousPeriodStartDate &&
				        'Calendar'[Date] <= PreviousPeriodEndDate
				    )
				),0)-1

			formatStringDefinition = "+0.0%;-0.0%;0%"

		calculationItem 'PreviousPeriod Difference' =
				
				VAR SelectedStartDate = MIN('Calendar'[Date])
				VAR SelectedEndDate = MAX('Calendar'[Date])
				VAR DaysInSelectedPeriod = DATEDIFF(SelectedStartDate, SelectedEndDate, DAY) + 1
				VAR PreviousPeriodStartDate = SelectedStartDate - DaysInSelectedPeriod
				VAR PreviousPeriodEndDate = SelectedEndDate - DaysInSelectedPeriod
				RETURN
				SELECTEDMEASURE() - CALCULATE(
				    SELECTEDMEASURE(),
				    FILTER(
				        ALL('Calendar'[Date]),
				        'Calendar'[Date] >= PreviousPeriodStartDate &&
				        'Calendar'[Date] <= PreviousPeriodEndDate
				    )
				)

			formatStringDefinition = "+0.0%;-0.0%;0%"

